 <!-- Iterating through all the plants bookings -->
<% @bookings.each do |booking| %>
   <% if booking.review %>
    <div class="card p-4 mt-4">
      <%=  render partial: 'shared/reviews/review_stars_and_comment', collection: [booking], as: :booking %>
    </div>
  <% else %>
  <% end %>
<% end %>
